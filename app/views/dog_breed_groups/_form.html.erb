<%= form_for @dog_breed_group, :html => { role: 'form', :class => 'form-horizontal' } do |f| %>
  <div class="form-group">

    <%= f.label :name %>

      <%= f.text_field :name, :class => 'form-control' %>

  </div>


  <div class="form-group">
    <%= f.submit nil, :class => 'btn btn-primary', 'value'=>'Update' %> or
    <%= link_to 'Cancel', dog_breed_groups_path %>
  </div>
<% end %>
